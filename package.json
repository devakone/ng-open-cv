{
  "name": "ng-open-cv-lib",
  "version": "0.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href \"https://devakone.github.io/ng-open-cv/\"",
    "publish-site": "npm run build && npx ngh --dir=dist/ng-open-cv-lib",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build ng-open-cv",
    "build-lib-publish-site": "npm run package && npm run publish-site",
    "copy-lib-assets": "cp-cli projects/ng-open-cv/src/lib/assets dist/ng-open-cv/lib/assets",
    "copy-license": "cp LICENSE dist/ng-open-cv/",
    "copy-readme": "cp README.md dist/ng-open-cv/",
    "copy-files": "npm run copy-license && npm run copy-readme && npm run copy-lib-assets",
    "npm_pack": "cd dist/ng-open-cv && npm pack",
    "package": "npm run build-lib && npm run copy-files && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.13.8",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.3.0",
    "cp-cli": "^1.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "tar": ">=4.4.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-dev-server": ">=3.1.11"
  }
}
